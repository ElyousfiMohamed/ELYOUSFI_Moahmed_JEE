+            System.out.println("objectName "+c.getObjectName());
+            System.out.println("className "+c.getClassName());
+            System.out.println("propertyName "+c.getProperty().getName());
+            System.out.println("propertyRef "+c.getProperty().getRef());
+        }
+
+    }
+}
